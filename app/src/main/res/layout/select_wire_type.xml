<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:padding="20dp"
        android:text="@string/fourthTitle"
        android:textAlignment="center"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="20dp"
        android:text="@string/fourthText"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="395dp"
        android:layout_height="255dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="0px"
            android:layout_weight="0.25">

            <ToggleButton
                android:id="@+id/toggleButton1"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:checked="false"
                android:text="ToggleButton"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="271dp" />

            <ToggleButton
                android:id="@+id/toggleButton2"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:text="ToggleButton" />

            <ToggleButton
                android:id="@+id/toggleButton3"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:text="ToggleButton" />

            <ToggleButton
                android:id="@+id/toggleButton4"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:text="ToggleButton" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="0px"
            android:layout_weight="0.25">

            <ToggleButton
                android:id="@+id/toggleButton5"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:text="ToggleButton"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="319dp" />

            <ToggleButton
                android:id="@+id/toggleButton6"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:text="ToggleButton"
                tools:layout_editor_absoluteX="96dp"
                tools:layout_editor_absoluteY="318dp" />

            <ToggleButton
                android:id="@+id/toggleButton7"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:text="ToggleButton"
                tools:layout_editor_absoluteX="184dp"
                tools:layout_editor_absoluteY="319dp" />

            <ToggleButton
                android:id="@+id/toggleButton8"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:text="ToggleButton"
                tools:layout_editor_absoluteX="272dp"
                tools:layout_editor_absoluteY="319dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="0px"
            android:layout_weight="0.25">

            <ToggleButton
                android:id="@+id/toggleButton9"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:text="ToggleButton"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="319dp" />

            <ToggleButton
                android:id="@+id/toggleButton10"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:text="ToggleButton"
                tools:layout_editor_absoluteX="96dp"
                tools:layout_editor_absoluteY="318dp" />

            <ToggleButton
                android:id="@+id/toggleButton11"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:text="ToggleButton"
                tools:layout_editor_absoluteX="184dp"
                tools:layout_editor_absoluteY="319dp" />

            <ToggleButton
                android:id="@+id/toggleButton12"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:text="ToggleButton"
                tools:layout_editor_absoluteX="272dp"
                tools:layout_editor_absoluteY="319dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="0px"
            android:layout_weight="0.25">

            <ToggleButton
                android:id="@+id/toggleButton13"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:text="ToggleButton"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="319dp" />

            <ToggleButton
                android:id="@+id/toggleButton14"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:text="ToggleButton"
                android:textColorLink="@android:color/holo_blue_light"
                tools:layout_editor_absoluteX="96dp"
                tools:layout_editor_absoluteY="318dp" />

            <ToggleButton
                android:id="@+id/toggleButton15"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:text="ToggleButton"
                android:textColorLink="@android:color/holo_blue_light"
                tools:layout_editor_absoluteX="184dp"
                tools:layout_editor_absoluteY="319dp" />

            <ToggleButton
                android:id="@+id/toggleButton16"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@android:color/white"
                android:text="ToggleButton"
                android:textColorLink="@android:color/holo_blue_light"
                tools:layout_editor_absoluteX="272dp"
                tools:layout_editor_absoluteY="319dp" />
        </TableRow>
    </TableLayout>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:padding="20dp"
        android:textSize="18sp"
        android:text="@string/fourthTextLink"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="108dp"
        android:layout_marginEnd="108dp"
        android:layout_marginBottom="20dp"
        android:text="@string/fourthTextQues"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/fourthButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/fourthButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="144dp"
        android:layout_marginEnd="144dp"
        android:layout_marginBottom="41dp"
        android:text="@string/yesContinue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>